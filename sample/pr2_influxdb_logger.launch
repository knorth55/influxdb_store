<launch>
  <arg name="database" default="pr1040" />

  <include file="$(find influxdb_store)/launch/influxdb_logger.launch">
    <arg name="host" value="localhost" />
    <arg name="port" value="8086" />
    <arg name="database" value="$(arg database)" />
    <arg name="joint_states_topic" value="/joint_states" />
    <arg name="battery_states_topic" value="/battery/server2" />
    <arg name="map_frame_id" value="map" />
  </include>
</launch>
